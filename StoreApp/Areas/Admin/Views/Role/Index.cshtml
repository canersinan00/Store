@model IEnumerable<IdentityRole>

<div class="text-center my-5">
    <div class="display-6">Roles</div>
</div>

<ul class="list-group">
    @foreach (var role in Model)
    {
        <li class="list-group-item d-flex justify-content-between align-items-center">
            @role.Name
        </li>
    }
</ul>